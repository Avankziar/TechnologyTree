Rewards:
  UnlockableInteractions:
  - block_place:DIRT:null:CanAccess:TTExp:Money:VanillaExp:CustomCommand #Für jeden Wert, Kann interagieren, TTExp, Money & VanillaExp wird mit dem Event und dem Material dann ein BonusMalus erzeugt.
  - block_break:DIRT:null:canAccess=true:ttexp=0.5:vault=1.0:vaexp=20:cmd=<cmd>=1.0 #TTExp = 0.5, Money Vault = 1.0, VanillaExp = 20, customcmd = 1.0 (Mit %value% und %player% replacer als consolecommand
  DropChance: #Für jede DropAnzahl wird mit Event und Material
  - block_break:DIRT:null:mat=DIRT:1:1.0 #Gibt 100% Chance, dass beim BlockAbbau von Dirt, 1 DirtItem droppt. Mehrfache DropChange des gleichen Blocks & Event addieren sich.
  - block_break:DIRT:null:mat=DIRT:2:0.10 #DropSystem umschreiben dahingegend, dass Verz. Glück & Loot miteinberechnet werden.
  - block_break:DIRT:null:item=Sword:1:10.0
  Command:
  - spigot:player:/warp %player% #Mit Replacer für Spielernamen
  - bungee:console:/do do #Für Console oder als Spieler
  Item:
  - sword_y:64 #Item y wird aus dem ItemOrdner geholt und hier eingesetzt.
  BonusMalus:
  - bonusmalusname:ADDITION:5 #Verteilt BonusMalus
  - bonusmalusname:MULTIPLICATION:1.1
  ConditionEntry:
  - conditionname:Wert # Wert kann boolean, int/double oder text sein.
  