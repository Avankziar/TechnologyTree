Rewards:
  UnlockableInteractions:
  #Für jeden Wert, Kann interagieren, TTExp, Money & VanillaExp wird mit dem Event und dem Material dann ein BonusMalus erzeugt.
  - block_place:DIRT:null:CanAccess:TTExp:Money:VanillaExp:CustomCommand
  #TTExp = 0.5, Money Vault = 1.0, VanillaExp = 20, customcmd = 1.0 (Mit %value% und %player% replacer als consolecommand
  - block_break:DIRT:null:canAccess=true:ttexp=0.5:vault=1.0:vaexp=20:cmd=spigot:give %value %DIAMOND %player%=1.0 
  UnlockableRecipe:
  - BLASTING:<Filename> //RecipeType, und der Filename des Rezeptes
  DropChance: #Für jede DropAnzahl wird mit Event und Material
  - block_break:DIRT:null:mat=DIRT:1:1.0 #Gibt 100% Chance, dass beim BlockAbbau von Dirt, 1 DirtItem droppt. Mehrfache DropChange des gleichen Blocks & Event addieren sich.
  - block_break:DIRT:null:mat=DIRT:2:0.10 #DropSystem umschreiben dahingegend, dass Verz. Glück & Loot miteinberechnet werden.
  - block_break:DIRT:null:item=Sword:1:10.0
  SilkTouchDropChance: #Nur, wenn man mit einem Werkzeug arbeitet, welches SilkTouch hat
  - block_break:DIRT:null:mat=DIRT:1:1.0 #Gibt 100% Chance, dass beim BlockAbbau von Dirt, 1 DirtItem droppt. Mehrfache DropChange des gleichen Blocks & Event addieren sich.
  - block_break:DIRT:null:mat=DIRT:2:0.10 #DropSystem umschreiben dahingegend, dass Verz. Glück & Loot miteinberechnet werden.
  - block_break:DIRT:null:item=Sword:1:10.0
  
  Command:
  - spigot:value:/warp %player% %value% #Mit Replacer für Spielernamen und den Anzahlswert
  - bungee:value:/do do #Für Console
  Item:
  - sword_y:64 #Item y wird aus dem ItemOrdner geholt und hier eingesetzt.
  Modifier:
  - modifiername:ADDITION:5:<internreason>:<displayreason>:<dd-HH-mm> #Verteilt Modifier
  - modifiername:MULTIPLICATION:1.1
  ValueEntry:
  - valuename:<Wert>:<internreason>:<displayreason>:<dd-HH-mm> # Wert kann boolean, int/double oder text sein.
  